#include "TREX.nddl"
#include "platform.nddl"

class Navigator extends AgentTimeline {
	
	Navigator(Mode _mode) {
		super(_mode, "Idle", true);
	}
    
	predicate Idle {}
	
	action Going {
		float latitude, longitude, z, speed, dist;
		float orig_lat, orig_lon, orig_z;		
	}
	
	predicate At {
		float latitude, longitude, z, speed;
		int secs;		
	}
}

