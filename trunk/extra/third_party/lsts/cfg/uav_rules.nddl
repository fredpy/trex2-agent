Navigator::Visit{
	//met_by(Idle i);

	contains(effect Reference.Going s);
	s.latitude == latitude;
	s.longitude == longitude;
	s.radius == -100.0;
}


ReferenceState::Going{
	if(near_xy == true){
      starts_before(effect Reference.Surveil s1); // Why does it crash if I use starts instead of starts_before?
        s1.start < start + 20;
        s1.end < s1.start + 100;
	} 
}

