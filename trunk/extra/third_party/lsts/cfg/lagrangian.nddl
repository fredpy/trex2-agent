class LagrangianSpace extends Timeline {
    predicate Holds {
	float origin_lat, origin_lon;
	float cos_angle, sin_angle;
	float speed, _speed_2;
	bool dynamic;
	
	// _speed_2 == speed^2
	speed == sqrt(_speed_2); 
	// 1.0 == cos_angle^2 + sin_angle^2
	calcDistance(1.0, 0.0, 0.0, cos_angle, sin_angle);
	cos_angle <= 1.0;
	-1.0 <= cos_angle;
	sin_angle <= 1.0;
	-1.0 <= sin_angle;

	start + duration == end;
    }
    LagrangianSpace() {}
}

