#include "declarations.nddl"
#include "platform.nddl"

class VehicleControl;

class Estimator extends AgentTimeline {
	
	Estimator(Mode _mode) {
		super(_mode, "Boot");
	}
	
    Estimator(Mode _mode, bool plan) {
	super(_mode, plan);
	}

	predicate Boot {}
	
	predicate At {
		float latitude, longitude, lat_deg, lon_deg, depth, tolerance, speed;
		int secs;
		secs >= 0;
		tolerance >= 0;
    	duration == 1;
	}
}
