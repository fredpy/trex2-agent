<?xml version="1.0"?>

<Goal on="estimator" pred="At">
  <Variable name="start">
    <!-- Example of a date range. Dates are specified in the format
         YYYY-MMM-DD HH:MM:SS.mmm 
	 and are always UTC -->
    <date min="2011-Jul-10 10:00:00.500-05:00" max="2020-Jul-10 22:00:00.500" />
  </Variable>
  <Variable name="lat_deg">
    <float min="41.185279" max="41.185279" />
  </Variable>
  <Variable name="lon_deg">
    <float min="-8.705758" max="-8.705758" />
  </Variable>
  <Variable name="depth">
    <float min="2" max="2"/>
  </Variable>
  <Variable name="secs">
    <!-- Example of duration. Dates are specified using the format 
         HH:MM:SS.mmm
    -->
    <duration min="0:00:05.3" max="1:00:00" />
  </Variable>
</Goal>